CREATE TABLE clients(
  id SERIAL PRIMARY KEY,
  first_name VARCHAR(100),
  last_name VARCHAR(100),
  age NUMERIC(4),
  city VARCHAR(100),
  date_of_registry DATE,
  date_of_birth DATE
);

CREATE TABLE purchases(
  id SERIAL PRIMARY KEY,
  product VARCHAR(100),
  quantity NUMERIC(10),
  client_id NUMERIC(10),
  date_of_purchasing DATE,
  city VARCHAR(100)
);
